<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatisd.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace="인터페이스의 경로+이름' -->	
<mapper namespace="com.human.app.iBook">
	<insert id="doAddBook">
		insert into booking(roomcode,roompriceall,bookingpeople,bookingdate,bookingname,mobile) 
		values(seq_book.nextval,#{param1},#{param2},#{param3},#{param4},#{param5})
	</insert>
	<delete id="doDeleteBook">
		delete from booking where roomname=#{param1}
	</delete>
</mapper>